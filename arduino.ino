// Array of all bitmaps for convenience. (Total bytes used to store images in PROGMEM = 2080)
const int myBitmapallArray_LEN = 4;
const unsigned char* myBitmapallArray[myBitmapallArray_LEN] = {
	myBitmapdm_emiko,
	epd_bitmap_bad_apple_img,
	epd_bitmap_hoshino_img,
	epd_bitmap_osu_logo,
};

int backgroundNum = 0;

void setup(void) {
  u8g2.begin();
	Serial.begin(9600);
    // Khởi tạo nút bấm
  for (int i = 0; i < 4; i++) {
    pinMode(buttonPins[i], INPUT_PULLUP);  // Dùng internal pull-up
  }

	if (digitalRead(4) == LOW) {
		Serial.println("Config mode detected");
		configState = true;
	}

	u8g2.clearBuffer();
	EEPROM.get(0, backgroundNum);
	u8g2.drawXBMP(0, 0, 128, 64, epd_bitmap_firm_logo);
	u8g2.sendBuffer();
	delay(2000);
}

void loop(void) {
	int n1; int n2;
	EEPROM.get(0, n1);
	EEPROM.get(2, n2);
	if (n1 == -1) {
		EEPROM.put(0, 0);
	}
	if (n2 == -1) {
		EEPROM.put(2, 0);
	}
	while (configState) {
		setConfig();
	} 
	if (digitalRead(4) != LOW && updateScreen) {
		u8g2.clearBuffer();
		EEPROM.get(0, backgroundNum);
		u8g2.drawXBMP(0, 0, 128, 64, myBitmapallArray[backgroundNum]);
		u8g2.sendBuffer();
		updateScreen = false;
	}
	int osuKeyNum = 0;
	EEPROM.get(2, osuKeyNum);
  for (int i = 0; i < 4; i++) {
    int reading = digitalRead(buttonPins[i]);

    if (reading == LOW && !buttonState[i]) {
      // Nút được nhấn
      Keyboard.press(osukeys[osuKeyNum][i]);
      buttonState[i] = true;
      delay(10);
    } 
    else if (reading == HIGH && buttonState[i]) {
      // Nút được nhả
      Keyboard.release(osukeys[osuKeyNum][i]);
      buttonState[i] = false;
      delay(10);
    }
  }
}

void setConfig() {
	char* modes[] = {"","Background", "Modes", "Info", "Exit", ""};
	char* backgrounds[] = {"", "Dm Emiko Nin", "Bad Apple", "Hoshino", "osu! Logo", ""};
	char* moes[] = {"", "osu!", "osu! 2", "osu! mania", "osu! taiko", "osu! catch", ""};
	int mode = 1;
	bool needUpdate = true;
	bool backgroundMode = false;
	bool moeMode = false;
	int len = (sizeof(modes) / sizeof(modes[0])) - 2;

	while (true) {
		if (needUpdate) {
			u8g2.clearBuffer();

			u8g2.drawXBMP(0, 0, 128, 64, myBitmapmenu);
			u8g2.setFont(u8g2_font_7x13B_tr);

			u8g2.drawStr(0,10, modes[mode - 1]);
			u8g2.drawStr(20,35, modes[mode]);
			u8g2.drawStr(0,60, modes[mode + 1]);
			u8g2.sendBuffer();


			needUpdate = false;
		}
		if (digitalRead(6) == LOW && mode < len) {
			mode += 1;
			needUpdate = true;
			delay(100);
		}
		if (digitalRead(5) == LOW && mode > 1) {
			mode -= 1;
			needUpdate = true;
			delay(100);
		}
		if (digitalRead(7) == LOW) {
			if (backgroundMode) {
				EEPROM.put(0, mode - 1);
				u8g2.clearBuffer();
				u8g2.drawXBMP(0, 0, 128, 64, myBitmapallArray[mode - 1]);
				u8g2.sendBuffer();
				delay(2000);
				configState = true;
				delay(100);
				return;
			}
			if (moeMode) {
				EEPROM.put(2, mode - 1);
				configState = true;
				delay(100);
				return;
			}
			else {
				if (mode == 1) {
					for (int i = 0; i < (sizeof(backgrounds) / sizeof(backgrounds[0])); i++) {
						modes[i] = backgrounds[i];
					}
					mode = 1;
					needUpdate = true;
					backgroundMode = true;
					len = (sizeof(backgrounds) / sizeof(backgrounds[0])) - 2;
					delay(100);
				}
				if (mode == 2) {
					for (int i = 0; i < (sizeof(moes) / sizeof(moes[0])); i++) {
						modes[i] = moes[i];
					}
					mode = 1;
					needUpdate = true;
					moeMode = true;
					len = (sizeof(moes) / sizeof(moes[0])) - 2;
					delay(100);
				}
				if (mode == 3) {
					u8g2.clearBuffer();
					u8g2.drawXBMP(0, 0, 96, 48, epd_bitmap_firm_logo_small);
					u8g2.setFont(u8g2_font_5x7_tf);
					u8g2.drawStr(0, 48, "Made by BiaHaNoi");
					u8g2.drawStr(0, 57, "Inspired by DU ORIGIN");
					u8g2.sendBuffer();
					delay(3000);
					return;
				}
				if (mode == 4) {
					configState = false;
					return;
				}
			}
		}
	}
}
